{
  "scripts": {
    "lint": "eslint . --ext .js --ext .jsx --cache --ignore-path .eslintignore",
    "lint:force": "eslint . --ext .js --ext .jsx --ignore-path .eslintignore",
    "lint:staged": "lint-staged",
    "format": "prettier --write --trailing-comma all {app,server}/**/*.{js,jsx}",
    "precommit": "yarn run lint:staged"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-react-native": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "prettier": "^1.8.2"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write --trailing-comma all",
      "git add"
    ]
  },
  "name": "ucl-assistant",
  "version": "0.1.0",
  "description": "An app to manage your UCL life",
  "author": "Matt Bell and UCL API",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.1.2"
  }
}
